<div id="header">
  <div id="inner-header">
    <div class="wrapper-l">
      <div class="row-fluid">
        <span class="span2">
          <%= link_to root_url do %>
            PoExplorer <span class="beta">/ beta</span>
          <% end %>
        </span>

        <span class="span4">

          <ul class="clearfix main-menu">
            <li class="tn-search">
              <%= link_to "Items", new_search_path %>
            </li>
            <% if false %>
              <li class="tn-builds">
                <%= link_to "Builds", builds_path %>
              </li>
            <% end %>

            <li class="dropdown menu-right">
              <%= link_to "", "#", class: "dropdown-toggle icon-caret-down ico", data: { toggle: "dropdown" } %>
              <ul class="dropdown-menu">
                <li><%= link_to "Help", search_by_price_path %></li>
              </ul>
            </li>

          </ul>
        </span>

        <span class="span6">
          <%= render "layouts/layout/header/user" %>
        </span>

      </div>
    </div>


  </div>
</div>
